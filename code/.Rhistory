View(df_eui)
# Continue sampling energy saving
mean_perc <- df_eui %>%
mutate(ref_saving = (ref - interv) / sqm,
rand_saving = (ref - rand) / sqm,
perc = rand_saving / ref_saving * 100) %>%
group_by(site) %>%
summarise(perc_mean = round(mean(perc), digits = 0),
pos = round(n() / 2) + 1)
View(mean_perc)
